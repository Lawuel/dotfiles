{
  "layer": "top",
  "position": "top",
  "modules-left": [
    "group/power",
    "idle_inhibitor",
    "bluetooth",
    "network",
    "mpris",
    "cava"
  ],
  "modules-center": ["hyprland/workspaces"],
  "modules-right": [
    "wireplumber",
    "backlight",
    "cpu",
    "memory",
    "user",
    "clock"
  ],

  "group/power": {
    "orientation": "horizontal",
    "modules": [
      "custom/show-power-options",
      "custom/suspend",
      "custom/hibernate",
      "custom/reboot",
      "custom/shutdown",
      "custom/logout"
    ],
    "drawer": {
      "click-to-reveal": true,
      "transition-left-to-right": true
    }
  },

  "custom/show-power-options": {
    "format": "󱐋",
    "tooltip": true,
    "tooltip-format": "Power Options"
  },

  "custom/suspend": {
    "format": "󰤄",
    "on-click": "systemctl suspend",
    "tooltip-format": "Suspend"
  },

  "custom/hibernate": {
    "format": "󰜗",
    "on-click": "systemctl hibernate",
    "tooltip-format": "Hibernate"
  },

  "custom/reboot": {
    "format": "󰜉",
    "on-click": "systemctl reboot",
    "tooltip-format": "Reboot"
  },

  "custom/shutdown": {
    "format": "󰐥",
    "on-click": "systemctl poweroff",
    "tooltip-format": "Shutdown"
  },

  "custom/logout": {
    "format": "󰍃",
    "on-click": "loginctl terminate-user \"\"",
    "tooltip-format": "Logout"
  },

  "idle_inhibitor": {
    "format": "{icon}",
    "format-icons": {
      "activated": "󰈈",
      "deactivated": "󰈉"
    },
    "tooltip-format-activated": "Activated",
    "tooltip-format-deactivated": "Deactivated"
  },

  "bluetooth": {
    "on-click": "uwsm-app -- blueman-manager",
    "format": "󰂯",
    "format-off": "󰂲",
    "format-disabled": "󰂲",
    "format-connected": "󰂱",
    "tooltip-format": "bluetooth",
    "tooltip-format-off": "Off",
    "tooltip-format-disabled": "Disabled",
    "tooltip-format-connected": "{device_alias}"
  },

  "network": {
    "on-click": "uwsm-app -- nm-connection-editor",
    "format-disconnected": "󰤮",
    "tooltip-format": "{essid}",
    "format": "{icon}",
    "format-icons": ["󰤟", "󰤢", "󰤥", "󰤨"]
  },

  "mpris": {
    "format": "{status_icon}",
    "status-icons": {
      "paused": "󰐊",
      "playing": "󰏤"
    },
    "tooltip-format": "{player}: {artist} | {title}"
  },

  "cava": {
    "cava_config": "/home/mano/.config/cava/config",
    "framerate": 30,
    "autosens": 1,
    "sensitivity": 1,
    "bars": 32,
    "lower_cutoff_freq": 50,
    "higher_cutoff_freq": 10000,
    "method": "pipewire",
    "source": "auto",
    "stereo": true,
    "reverse": false,
    "bar_delimiter": 0,
    "monstercat": false,
    "waves": false,
    "noise_reduction": 0,
    "input_delay": 2,
    "sleep_timer": 1,
    "hide_on_silence": true,
    "format-icons": [
      "🭹",
      "🭸",
      "🭹",
      "🭺",
      "🭹",
      "🭸",
      "🭷",
      "🭸",
      "🭹",
      "🭺",
      "🭻",
      "🭺",
      "🭹",
      "🭸",
      "🭷",
      "🭶",
      "🭷",
      "🭸",
      "🭹",
      "🭺",
      "🭻",
      "▁",
      "🭻",
      "🭺",
      "🭹",
      "🭸",
      "🭷",
      "🭶",
      "▔"
    ], //▁🭻🭺🭹🭸🭷🭶▔   ░▒▓█   🭹🭸🭹🭺🭹🭸🭷🭸🭹🭺🭻🭺🭹🭸🭷🭶🭷🭸🭹🭺🭻▁🭻🭺🭹🭸🭷🭶▔
    "actions": {
      "on-click-right": "mode"
    }
  },

  "hyprland/workspaces": {
    "all-outputs": true,
    "format": "{icon}",
    "format-icons": {
      "urgent": "󰳤",
      "active": "󰝤",
      "default": "󰝣"
    }
  },

  "wireplumber": {
    "on-click": "uwsm-app -- pavucontrol",
    "format-muted": "󰝟 {volume}%",
    "format": "{icon} {volume}%",
    "format-icons": ["󰕿", "󰖀", "󰕾"]
  },

  "backlight": {
    "device": "intel_backlight",
    "tooltip": false,
    "format": "{icon} {percent}%",
    "format-icons": ["󱩎", "󱩏", "󱩐", "󱩑", "󱩒", "󱩓", "󱩔", "󱩕", "󱩖", "󰛨"]
  },

  "cpu": {
    "interval": 15,
    "format": "󰙅 {usage}%"
  },

  "memory": {
    "interval": 15,
    "format": "󰌨 {percentage}%"
  },

  "user": {
    "format": "{work_H}:{work_M}"
  },

  "clock": {
    "format": "{:%I:%M}",
    "tooltip-format": "{:%a %b %d, %Y}"
  }
}
